{
  "name": "refacto-backend",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "tsc && prettier --check .",
    "build": "rm -rf build && rollup --config rollup.config.mjs && cp -r src/static build && chmod +x build/index.js",
    "build-watch": "rollup --watch --config rollup.config.mjs",
    "test": "lean-test --preprocess tsc --parallel"
  },
  "devDependencies": {
    "@collection-storage/dynamodb": "4.x",
    "@collection-storage/mongodb": "4.x",
    "@collection-storage/postgresql": "4.x",
    "@collection-storage/redis": "4.x",
    "@collection-storage/sqlite": "4.x",
    "@rollup/plugin-commonjs": "29.x",
    "@rollup/plugin-json": "6.x",
    "@rollup/plugin-node-resolve": "16.x",
    "@rollup/plugin-replace": "6.x",
    "@rollup/plugin-terser": "0.4.x",
    "@rollup/plugin-typescript": "12.x",
    "@tsconfig/strictest": "2.x",
    "@types/node": "20.x",
    "authentication-backend": "2.x",
    "collection-storage": "4.x",
    "json-immutability-helper": "4.0.x",
    "jwt-simple": "0.5.x",
    "lean-test": "2.x",
    "prettier": "3.7.4",
    "pwd-hasher": "2.x",
    "rollup": "4.x",
    "shared-reducer": "6.2.x",
    "supertest": "7.x",
    "superwstest": "2.x",
    "typescript": "5.9.x",
    "web-listener": "0.17.1",
    "ws": "8.x"
  }
}
