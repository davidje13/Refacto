{
  "name": "refacto-backend",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "tsc && prettier --check .",
    "build": "rm -rf build && rollup --config rollup.config.mjs && cp -r src/static policy.json build && chmod +x build/index.js",
    "start": "npm run build && NODE_ENV=development build/index.js",
    "test": "lean-test --preprocess tsc --parallel"
  },
  "dependencies": {
    "authentication-backend": "1.x",
    "collection-storage": "3.x",
    "express": "4.x",
    "express-static-gzip": "2.x",
    "ioredis": "5.x",
    "json-immutability-helper": "3.x",
    "jwt-simple": "0.5.x",
    "mongodb": "6.x",
    "pg": "8.x",
    "pwd-hasher": "2.x",
    "shared-reducer-backend": "3.x",
    "tslib": "2.6.x",
    "websocket-express": "3.x",
    "ws": "8.x"
  },
  "optionalDependencies": {
    "http-proxy-middleware": "*"
  },
  "devDependencies": {
    "@rollup/plugin-json": "6.x",
    "@rollup/plugin-node-resolve": "15.x",
    "@rollup/plugin-typescript": "11.x",
    "@tsconfig/strictest": "2.x",
    "@types/node": "20.x",
    "http-proxy-middleware": "3.x",
    "lean-test": "2.x",
    "prettier": "3.3.2",
    "rollup": "4.x",
    "supertest": "7.x",
    "superwstest": "2.x",
    "typescript": "5.4.x"
  }
}
